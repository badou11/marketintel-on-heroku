{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "marketintel": {
      "projectType": "application",
      "schematics": {},
      "root": "",
      "sourceRoot": "src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/marketintel",
            "index": "src/index.html",
            "main": "src/main.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "tsconfig.app.json",
            "aot": true,
            "assets": [
              "src/favicon.ico",
              "src/assets"
            ],
            "styles": [
              "src/styles.css",
              "node_modules/bootstrap/dist/css/bootstrap.min.css",
               "node_modules/semantic-ui-css/semantic.min.css",
              "node_modules/semantic-ui-css/components/button.min.css",
              "node_modules/semantic-ui-css/components/accordion.min.css",
              "node_modules/semantic-ui-css/components/ad.min.css",
              "node_modules/semantic-ui-css/components/breadcrumb.min.css",
              "node_modules/semantic-ui-css/components/card.min.css",
              "node_modules/semantic-ui-css/components/checkbox.min.css",
              "node_modules/semantic-ui-css/components/comment.min.css",
              "node_modules/semantic-ui-css/components/container.min.css",
              "node_modules/semantic-ui-css/components/dimmer.min.css",
              "node_modules/semantic-ui-css/components/divider.min.css",
              "node_modules/semantic-ui-css/components/dropdown.min.css",
              "node_modules/semantic-ui-css/components/embed.min.css",
              "node_modules/semantic-ui-css/components/feed.min.css",
              "node_modules/semantic-ui-css/components/flag.min.css",
              "node_modules/semantic-ui-css/components/form.min.css",
              "node_modules/semantic-ui-css/components/header.min.css",
              "node_modules/semantic-ui-css/components/grid.min.css",
              "node_modules/semantic-ui-css/components/icon.min.css",
              "node_modules/semantic-ui-css/components/image.min.css",
              "node_modules/semantic-ui-css/components/input.min.css",
              "node_modules/semantic-ui-css/components/item.min.css",
              "node_modules/semantic-ui-css/components/label.min.css",
              "node_modules/semantic-ui-css/components/list.min.css",
              "node_modules/semantic-ui-css/components/loader.min.css",
              "node_modules/semantic-ui-css/components/menu.min.css",
              "node_modules/semantic-ui-css/components/message.min.css",
              "node_modules/semantic-ui-css/components/modal.min.css",
              "node_modules/semantic-ui-css/components/nag.min.css",
              "node_modules/semantic-ui-css/components/placeholder.min.css",
              "node_modules/semantic-ui-css/components/popup.min.css",
              "node_modules/semantic-ui-css/components/progress.min.css",
              "node_modules/semantic-ui-css/components/rail.min.css",
              "node_modules/semantic-ui-css/components/rating.min.css",
              "node_modules/semantic-ui-css/components/reset.min.css",
              "node_modules/semantic-ui-css/components/reveal.min.css",
              "node_modules/semantic-ui-css/components/search.min.css",
              "node_modules/semantic-ui-css/components/segment.min.css",
              "node_modules/semantic-ui-css/components/shape.min.css",
              "node_modules/semantic-ui-css/components/sidebar.min.css",
              "node_modules/semantic-ui-css/components/site.min.css",
              "node_modules/semantic-ui-css/components/statistic.min.css",
              "node_modules/semantic-ui-css/components/step.min.css",
              "node_modules/semantic-ui-css/components/sticky.min.css",
              "node_modules/semantic-ui-css/components/tab.min.css",
              "node_modules/semantic-ui-css/components/table.min.css",
              "node_modules/semantic-ui-css/components/transition.min.css",
              "node_modules/semantic-ui-css/components/video.min.css"
            ],
            "scripts": [
              "node_modules/semantic-ui-css/semantic.min.js",
              "node_modules/semantic-ui-css/components/accordion.min.js",
              "node_modules/semantic-ui-css/components/checkbox.min.js",
              "node_modules/semantic-ui-css/components/colorize.min.js",
              "node_modules/semantic-ui-css/components/dimmer.min.js",
              "node_modules/semantic-ui-css/components/dropdown.min.js",
              "node_modules/semantic-ui-css/components/embed.min.js",
              "node_modules/semantic-ui-css/components/form.min.js",
              "node_modules/semantic-ui-css/components/modal.min.js",
              "node_modules/semantic-ui-css/components/nag.min.js",
              "node_modules/semantic-ui-css/components/popup.min.js",
              "node_modules/semantic-ui-css/components/progress.min.js",
              "node_modules/semantic-ui-css/components/rating.min.js",
              "node_modules/semantic-ui-css/components/search.min.js",
              "node_modules/semantic-ui-css/components/shape.min.js",
              "node_modules/semantic-ui-css/components/sidebar.min.js",
              "node_modules/semantic-ui-css/components/site.min.js",
              "node_modules/semantic-ui-css/components/state.min.js",
              "node_modules/semantic-ui-css/components/sticky.min.js",
              "node_modules/semantic-ui-css/components/tab.min.js",
              "node_modules/semantic-ui-css/components/transition.min.js",
              "node_modules/semantic-ui-css/components/video.min.js",
              "node_modules/semantic-ui-css/components/visit.min.js",
              "node_modules/semantic-ui-css/components/visibility.min.js"
            ]

          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "marketintel:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "marketintel:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "marketintel:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "src/test.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "tsconfig.spec.json",
            "karmaConfig": "karma.conf.js",
            "assets": [
              "src/favicon.ico",
              "src/assets"
            ],
            "styles": [
              "src/styles.css"
            ],
            "scripts": []
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "tsconfig.app.json",
              "tsconfig.spec.json",
              "e2e/tsconfig.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        },
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "e2e/protractor.conf.js",
            "devServerTarget": "marketintel:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "marketintel:serve:production"
            }
          }
        }
      }
    }},
  "defaultProject": "marketintel"
}
